<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mechanicProfile"
    android:background="@drawable/background">

    <ImageView
        android:id="@+id/mechanicProfile"
        android:layout_width="199dp"
        android:layout_height="180dp"
        android:src="@drawable/service"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.078" />

    <TextView
        android:id="@+id/mechanicEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adresa email"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mechanicUser"
        app:layout_constraintVertical_bias="0.133" />


    <TextView
        android:id="@+id/mechanicEmailOutput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adresa email"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mechanicUser"
        app:layout_constraintVertical_bias="0.133" />

    <TextView
        android:id="@+id/mechanicUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nume utilizator"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mechanicProfile"
        app:layout_constraintVertical_bias="0.163" />

    <TextView
        android:id="@+id/mechanicUserOutput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nume utilizator"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.806"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mechanicProfile"
        app:layout_constraintVertical_bias="0.163" />

    <TextView
        android:id="@+id/mechanicData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dara crearii"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mechanicEmail"
        app:layout_constraintVertical_bias="0.177" />

    <TextView
        android:id="@+id/mechanicDataOutput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parola"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.746"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mechanicEmail"
        app:layout_constraintVertical_bias="0.178" />

    <Button
        android:id="@+id/btnLogout"
        android:layout_width="189dp"
        android:layout_height="55dp"
        android:background="@drawable/btn_bg"
        android:text="Deconectare"
        android:textColor="@color/colorBlack"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mechanicProfile"
        app:layout_constraintVertical_bias="0.735" />


</androidx.constraintlayout.widget.ConstraintLayout>